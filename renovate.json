{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "digest"
      ],
      "enabled": false
    },
    {
      "matchPackagePatterns": [
        "^@inhouse/"
      ],
      "excludePackagePatterns": [
        "-flavor$"
      ],
      "groupName": "inhouse components-web monorepo"
    }
  ]
}
